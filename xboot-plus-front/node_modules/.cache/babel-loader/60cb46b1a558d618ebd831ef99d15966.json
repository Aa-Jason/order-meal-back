,CAXA;MA6EApB,QA7EA;MA6EA;MACAsB,QA9EA,CA8EA;;IA9EA;EAgFA,CAnFA;EAoFAC;IACAC,IADA,kBACA;MACA;IACA,CAHA;IAIAC,UAJA,sBAIAC,CAJA,EAIA;MACA;MACA;MACA;IACA,CARA;IASAC,cATA,0BASAD,CATA,EASA;MACA;MACA;IACA,CAZA;IAaAE,UAbA,sBAaAC,CAbA,EAaA;MACA;MACA;;MACA;QACA;MACA;;MACA;IACA,CApBA;IAqBAC,cArBA,4BAqBA;MACA;IACA,CAvBA;IAwBAC,YAxBA,wBAwBAF,CAxBA,EAwBA;MACA;IACA,CA1BA;IA2BAG,WA3BA,yBA2BA;MACA,oBADA,CAEA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;;MACA,aACA;QACAC,OADA;QAEAlC,+BAFA;QAGAmC,iCAHA;QAIAC;MAJA,CADA,EAOA;QACAF,OADA;QAEAlC,gCAFA;QAGAmC,iCAHA;QAIAC;MAJA,CAPA;MAcA;MACA;IACA,CA1DA;IA2DAC,GA3DA,iBA2DA;MACA;MACA;QAAAC;MAAA;MACA;QACA;QACAtC,WAFA;QAGAuC;MAHA;IAKA,CAnEA;IAoEAC,IApEA,gBAoEAb,CApEA,EAoEA;MACA;MACA;QAAAO;QAAAI;MAAA;MACA;QACA;QACAtC,YAFA;QAGAuC;MAHA;IAKA,CA5EA;IA6EAE,MA7EA,kBA6EAd,CA7EA,EA6EA;MAAA;;MACA;QACAb,aADA;QAEA;QACA4B,kCAHA;QAIAvC,aAJA;QAKAwC;UACA;UACA;UACA;UACA;UACA;UACA;UACA;UACA;UACA;UACA;UACA;;UACA;;UACA;;UACA;QACA;MApBA;IAsBA,CApGA;IAqGAC,MArGA,oBAqGA;MAAA;;MACA;QACA;QACA;MACA;;MACA;QACA9B,aADA;QAEA4B,wDAFA;QAGAvC,aAHA;QAIAwC;UACA;;UACA;YACAE;UACA,CAFA;;UAGAA,uCALA,CAMA;UACA;UACA;UACA;UACA;UACA;UACA;UACA;UACA;UACA;;UACA;;UACA;;UACA;;UACA;QACA;MAxBA;IA0BA;EApIA,CApFA;EA0NAC,OA1NA,qBA0NA;IACA;EACA;AA5NA","names":["name","data","openTip","loading","searchForm","pageNumber","pageSize","sort","order","selectList","columns","type","width","align","title","key","sortable","sortType","render","h","on","click","props","total","methods","init","changePage","v","changePageSize","changeSort","e","clearSelectAll","changeSelect","getDataList","id","createTime","updateTime","add","backRoute","query","edit","remove","content","onOk","delAll","ids","mounted"],"sourceRoot":"src/views/xboot-vue-template/new-window","sources":["newWindow.vue"],"sourcesContent":["<style lang=\"less\">\r\n@import \"@/styles/table-common.less\";\r\n</style>\r\n<template>\r\n  <div class=\"search\">\r\n    <Card>\r\n      <Row class=\"operation\">\r\n        <Button @click=\"add\" type=\"primary\" icon=\"md-add\">新窗口中添加</Button>\r\n        <Button @click=\"delAll\" icon=\"md-trash\">批量删除</Button>\r\n        <Button @click=\"getDataList\" icon=\"md-refresh\">刷新</Button>\r\n        <Button type=\"dashed\" @click=\"openTip = !openTip\">{{\r\n          openTip ? \"关闭提示\" : \"开启提示\"\r\n        }}</Button>\r\n      </Row>\r\n      <Alert show-icon v-show=\"openTip\">\r\n        已选择\r\n        <span class=\"select-count\">{{ selectList.length }}</span> 项\r\n        <a class=\"select-clear\" @click=\"clearSelectAll\">清空</a>\r\n      </Alert>\r\n      <Table\r\n        :loading=\"loading\"\r\n        border\r\n        :columns=\"columns\"\r\n        :data=\"data\"\r\n        ref=\"table\"\r\n        sortable=\"custom\"\r\n        @on-sort-change=\"changeSort\"\r\n        @on-selection-change=\"changeSelect\"\r\n      ></Table>\r\n      <Row type=\"flex\" justify=\"end\" class=\"page\">\r\n        <Page\r\n          :current=\"searchForm.pageNumber\"\r\n          :total=\"total\"\r\n          :page-size=\"searchForm.pageSize\"\r\n          @on-change=\"changePage\"\r\n          @on-page-size-change=\"changePageSize\"\r\n          :page-size-opts=\"[10, 20, 50]\"\r\n          size=\"small\"\r\n          show-total\r\n          show-elevator\r\n          show-sizer\r\n        ></Page>\r\n      </Row>\r\n    </Card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"new-window\",\r\n  data() {\r\n    return {\r\n      openTip: true, // 显示提示\r\n      loading: true, // 表单加载状态\r\n      searchForm: {\r\n        // 搜索框对应data对象\r\n        pageNumber: 1, // 当前页数\r\n        pageSize: 10, // 页面大小\r\n        sort: \"createTime\", // 默认排序字段\r\n        order: \"desc\", // 默认排序方式\r\n      },\r\n      selectList: [], // 多选数据\r\n      columns: [\r\n        // 表头\r\n        {\r\n          type: \"selection\",\r\n          width: 60,\r\n          align: \"center\",\r\n        },\r\n        {\r\n          type: \"index\",\r\n          width: 60,\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: \"名称\",\r\n          key: \"name\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          title: \"创建时间\",\r\n          key: \"createTime\",\r\n          sortable: true,\r\n          sortType: \"desc\",\r\n        },\r\n        {\r\n          title: \"更新时间\",\r\n          key: \"updateTime\",\r\n          sortable: true,\r\n        },\r\n        {\r\n          title: \"操作\",\r\n          key: \"action\",\r\n          align: \"center\",\r\n          width: 250,\r\n          render: (h, params) => {\r\n            return h(\"div\", [\r\n              h(\r\n                \"a\",\r\n                {\r\n                  on: {\r\n                    click: () => {\r\n                      this.edit(params.row);\r\n                    },\r\n                  },\r\n                },\r\n                \"新窗口中编辑\"\r\n              ),\r\n              h(\"Divider\", {\r\n                props: {\r\n                  type: \"vertical\",\r\n                },\r\n              }),\r\n              h(\r\n                \"a\",\r\n                {\r\n                  on: {\r\n                    click: () => {\r\n                      this.remove(params.row);\r\n                    },\r\n                  },\r\n                },\r\n                \"删除\"\r\n              ),\r\n            ]);\r\n          },\r\n        },\r\n      ],\r\n      data: [], // 表单数据\r\n      total: 0, // 表单数据总数\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.getDataList();\r\n    },\r\n    changePage(v) {\r\n      this.searchForm.pageNumber = v;\r\n      this.getDataList();\r\n      this.clearSelectAll();\r\n    },\r\n    changePageSize(v) {\r\n      this.searchForm.pageSize = v;\r\n      this.getDataList();\r\n    },\r\n    changeSort(e) {\r\n      this.searchForm.sort = e.key;\r\n      this.searchForm.order = e.order;\r\n      if (e.order == \"normal\") {\r\n        this.searchForm.order = \"\";\r\n      }\r\n      this.getDataList();\r\n    },\r\n    clearSelectAll() {\r\n      this.$refs.table.selectAll(false);\r\n    },\r\n    changeSelect(e) {\r\n      this.selectList = e;\r\n    },\r\n    getDataList() {\r\n      this.loading = true;\r\n      // 请求后端获取表单数据 请自行修改接口\r\n      // this.getRequest(\"请求路径\", this.searchForm).then(res => {\r\n      //   this.loading = false;\r\n      //   if (res.success) {\r\n      //     this.data = res.result.content;\r\n      //     this.total = res.result.totalElements;\r\n      //     if (this.data.length == 0 && this.searchForm.pageNumber > 1) {\r\n      //       this.searchForm.pageNumber -= 1;\r\n      //       this.getDataList();\r\n      //     }\r\n      //   }\r\n      // });\r\n      // 以下为模拟数据\r\n      this.data = [\r\n        {\r\n          id: \"1\",\r\n          name: \"XBoot(新窗口提交自动关闭后可返回该页面)\",\r\n          createTime: \"2018-08-08 00:08:00\",\r\n          updateTime: \"2018-08-08 00:08:00\",\r\n        },\r\n        {\r\n          id: \"2\",\r\n          name: \"Exrick(新窗口提交自动关闭后可返回该页面)\",\r\n          createTime: \"2018-08-08 00:08:00\",\r\n          updateTime: \"2018-08-08 00:08:00\",\r\n        },\r\n      ];\r\n      this.total = this.data.length;\r\n      this.loading = false;\r\n    },\r\n    add() {\r\n      // 记录返回路由\r\n      let query = { backRoute: this.$route.name };\r\n      this.$router.push({\r\n        // 该路由已在/router/router.js中定义好\r\n        name: \"add\",\r\n        query: query,\r\n      });\r\n    },\r\n    edit(v) {\r\n      // 记录返回路由\r\n      let query = { id: v.id, backRoute: this.$route.name };\r\n      this.$router.push({\r\n        // 该路由已在/router/router.js中定义好 携带id参数\r\n        name: \"edit\",\r\n        query: query,\r\n      });\r\n    },\r\n    remove(v) {\r\n      this.$Modal.confirm({\r\n        title: \"确认删除\",\r\n        // 记得确认修改此处\r\n        content: \"您确认要删除 \" + v.name + \" ?\",\r\n        loading: true,\r\n        onOk: () => {\r\n          // 删除\r\n          // this.deleteRequest(\"请求地址，如/deleteByIds/\" + v.id).then(res => {\r\n          //   this.$Modal.remove();\r\n          //   if (res.success) {\r\n          //     this.$Message.success(\"操作成功\");\r\n          //     this.clearSelectAll();\r\n          //     this.getDataList();\r\n          //   }\r\n          // });\r\n          // 模拟请求成功\r\n          this.$Message.success(\"操作成功\");\r\n          this.clearSelectAll();\r\n          this.$Modal.remove();\r\n          this.getDataList();\r\n        },\r\n      });\r\n    },\r\n    delAll() {\r\n      if (this.selectList.length <= 0) {\r\n        this.$Message.warning(\"您还未选择要删除的数据\");\r\n        return;\r\n      }\r\n      this.$Modal.confirm({\r\n        title: \"确认删除\",\r\n        content: \"您确认要删除所选的 \" + this.selectList.length + \" 条数据?\",\r\n        loading: true,\r\n        onOk: () => {\r\n          let ids = \"\";\r\n          this.selectList.forEach(function (e) {\r\n            ids += e.id + \",\";\r\n          });\r\n          ids = ids.substring(0, ids.length - 1);\r\n          // 批量删除\r\n          // this.deleteRequest(\"请求地址，如/deleteByIds/\" + ids).then(res => {\r\n          //   this.$Modal.remove();\r\n          //   if (res.success) {\r\n          //     this.$Message.success(\"操作成功\");\r\n          //     this.clearSelectAll();\r\n          //     this.getDataList();\r\n          //   }\r\n          // });\r\n          // 模拟请求成功\r\n          this.$Message.success(\"操作成功\");\r\n          this.$Modal.remove();\r\n          this.clearSelectAll();\r\n          this.getDataList();\r\n        },\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.init();\r\n  },\r\n};\r\n</script>"]}]}